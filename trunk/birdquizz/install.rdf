<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#"
     xmlns:songbird="http://www.songbirdnest.com/2007/addon-metadata-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>BirdQuizz@songbirdnest.com</em:id>
    <em:type>2</em:type>
    <em:name>BirdQuizz</em:name>
    <em:version>0.7</em:version>
    <em:creator>Mitch, GeekShadow</em:creator>
    <em:contributor>ventuno - Italian (it-IT) localisation</em:contributor>
    <em:description>Music quiz game, featuring your own music collection.</em:description>
    <em:iconURL>chrome://birdquizz/skin/icon.png</em:iconURL>
    <em:homepageURL>http://code.google.com/p/songbird-birdquizz/</em:homepageURL>
    <em:optionsURL>chrome://birdquizz/content/optionsWindow.xul</em:optionsURL>

    <!-- Songbird -->
    <em:targetApplication>
      <Description>
        <em:id>songbird@songbirdnest.com</em:id>
        <em:minVersion>0.5</em:minVersion>
        <em:maxVersion>0.7pre</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Register a custom media page.
         See http://songbirdnest.com/add-on-api/articles/media-pages -->
    <songbird:mediaPage>
      <Description>
        <songbird:contentTitle>BirdQuizz</songbird:contentTitle>
        <songbird:contentUrl>chrome://birdquizz/content/media-page.xul</songbird:contentUrl>
      </Description>
    </songbird:mediaPage>
  </Description>
</RDF>
